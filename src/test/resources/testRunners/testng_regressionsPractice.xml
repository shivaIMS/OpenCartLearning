		<parameter name="url" value="https://www.google.com"></parameter>
		<parameter name="browser" value="chrome"></parameter>
		<parameter name="headless" value="false"></parameter>
		<classes>
			<class name="testClasses.GoogleTest" />
		</classes>
	</test>
	<test name="OpenKart Test">
		<parameter name="url"
			value="https://naveenautomationlabs.com/opencart/index.php?route=account/login"></parameter>
		<parameter name="browser" value="firefox"></parameter>
		<parameter name="headless" value="false"></parameter>
		<classes>
			<class name="testClasses.OpenKartTest"></class>
		</classes>
	</test>
</suite> <!--
